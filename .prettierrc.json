{
  "semi": true,
  "singleQuote": true,
  "printWidth": 100,
  "tabWidth": 2,
  "trailingComma": "es5",
  "proseWrap": "never",
  "endOfLine": "lf",
  "plugins": ["@ianvs/prettier-plugin-sort-imports", "prettier-plugin-tailwindcss"],
  "importOrder": [
    "<BUILTIN_MODULES>",
    "",
    "^payload$",
    "^@payloadcms/db-(.*)$",
    "^@payloadcms/(?!plugin-)(.*)$",
    "^@payloadcms/plugin-(.*)$",
    "",
    "^(react/(.*)$)|^(react$)",
    "^(next/(.*)$)|^(next$)",
    "<THIRD_PARTY_MODULES>",
    "",
    "^@/app/(.*)$",
    "^@/blocks/(.*)$",
    "^@/components/(.*)$",
    "^@/collections/(.*)$",
    "^@/data/(.*)$",
    "^@/hooks/(.*)$",
    "^@/lib/(.*)$",
    "^@/endpoints/(.*)$",
    "^@/globals/(.*)$",
    "^@/heros/(.*)$",
    "^@/i18n/(.*)$",
    "^@/plugins/(.*)$",
    "^@/providers/(.*)$",
    "^@/utilities/(.*)$",
    "^@/search/(.*)$",
    "^@/payload-types/(.*)$",
    "^@/(?!app|collections|components|data|hooks|lib|utilities|search|styles|types)(.*)$",
    "",
    "^@/styles/(.*)$",
    "",
    "^@/types/(.*)$",
    "",
    "^[./]"
  ],
  "tailwindConfig": "./tailwind.config.mjs",
  "tailwindFunctions": ["clsx", "cn"]
}
